{
  "expo": {
    "name": "김포도시관리공사 e-캠퍼스",
    "slug": "gimpo-e-campus",
    "version": "1.0.3",
    "versionCode": 4,
    "orientation": "portrait",
    "icon": "./assets/icon.png",
